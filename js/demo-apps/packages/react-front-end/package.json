{
  "name": "@lunasec/react-front-end",
  "private": true,
  "version": "1.0.7",
  "license": "Apache-2.0",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:url": "if [ -z ${APPLICATION_FRONT_END+x} ]; then echo 'http://localhost:3000'; else echo $APPLICATION_FRONT_END; fi",
    "test:e2e": "cypress run --record --key 77270197-498a-4a68-b9ff-e5d6a0f04617 --browser chrome $TEST_OPTIONS --config baseUrl=$(yarn run --silent test:url)",
    "test:e2e:docker": "Xvfb :99 & TEST_OPTIONS=\"--headed\" yarn run test:e2e",
    "test:open": "cypress open --browser chrome --config baseUrl=$(yarn run --silent test:url)",
    "eject": "react-scripts eject",
    "prettier": "prettier \"*.{js,json,yml,md}\" \"src/**/*\"",
    "format": "npm run lint -- --fix",
    "format:check": "npm run lint",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src"
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@aws-sdk/credential-provider-ini": "^3.31.0",
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "@lunasec/browser-common": "^1.0.7",
    "@lunasec/demo-back-end": "^1.0.7",
    "@lunasec/react-sdk": "^1.0.7",
    "@lunasec/tokenizer-sdk": "^1.0.7",
    "@mui/icons-material": "^5.2.0",
    "@mui/material": "^5.2.2",
    "@mui/styles": "^5.2.2",
    "cypress-log-to-output": "^1.1.2",
    "cypress-terminal-report": "^3.3.2",
    "easy-peasy": "^5.0.3",
    "graphql-tag": "^2.12.5",
    "husky": "^6.0.0",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "rimraf": "^3.0.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "cypress": "8.0.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-iframe": "^1.0.1",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-order": "^2.1.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.2",
    "jest": "26.6.0",
    "lerna": "^3.22.1",
    "lint-staged": "^10.5.4",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
